{
  "name": "git-switch",
  "version": "1.2.1",
  "description": "Cross-platform electron app for managing git users while pair/mob programming",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:compile",
    "build:clean": "rimraf ./src/build",
    "build:compile": "webpack",
    "lint": "npm run lint:main && npm run lint:renderer && npm run lint:cli",
    "lint:cli": "eslint ./src/cli",
    "lint:main": "eslint ./src/common",
    "lint:renderer": "eslint ./src/client",
    "make:all": "npm run make:macos && npm run make:deb && npm run make:rpm && npm run make:windows",
    "make:deb": "electron-forge make --arch=x64 --platform=linux --targets=@electron-forge/maker-deb",
    "make:rpm": "electron-forge make --arch=x64 --platform=linux --targets=@electron-forge/maker-rpm",
    "make:macos": "electron-forge make --arch=x64 --platform=darwin",
    "make:windows": "electron-forge make --arch=x64 --platform=win32",
    "package:all": "npm run package:macos && npm run package:linux && npm run package:windows",
    "package:linux": "electron-forge package --arch=x64 --platform=linux",
    "package:macos": "electron-forge package --arch=x64 --platform=darwin",
    "package:windows": "electron-forge package --arch=x64 --platform=win32",
    "packages:zip": "node ./scripts/package-zipper",
    "precommit": "npm run lint && npm run test && npm run build",
    "release:clean": "rimraf ./out",
    "release:create": "npm run release:clean && npm run build && npm run package:all && npm run packages:zip",
    "release:publish": "node ./scripts/release-publisher",
    "start": "electron-forge start",
    "start:macos": "npm run build && npm run package:macos && open out/git-switch-darwin-x64/git-switch.app",
    "start:dev": "npm run build && cross-env NODE_ENV=dev npm run start",
    "test": "mocha",
    "test:watch": "npm run test -- --watch"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/git-switch-electron"
  },
  "dependencies": {
    "@babel/core": "7.9.0",
    "@babel/register": "7.9.0",
    "core-js": "3.6.4",
    "electron-squirrel-startup": "1.0.0",
    "lodash.capitalize": "4.2.1",
    "lodash.orderby": "4.6.0",
    "md5": "2.2.1",
    "menubar": "8.0.1",
    "uuid": "7.0.3",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "archiver": "3.1.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "electron": "8.2.0",
    "electron-connect": "0.6.3",
    "electron-connect-webpack-plugin": "0.1.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-jsx": "8.1.0",
    "eslint-config-standard-react": "9.2.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-mocha": "6.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-standard": "4.0.1",
    "html-webpack-plugin": "4.0.4",
    "husky": "4.2.3",
    "mini-css-extract-plugin": "0.9.0",
    "mocha": "7.1.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "sinon": "9.0.1",
    "sinon-chai": "3.5.0",
    "svg-react-loader": "0.4.6",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "appBundleId": "com.pluralsight.git-switch",
        "prune": true,
        "overwrite": true,
        "icon": "./src/assets/icons/icon",
        "ignore": [
          "\\.idea",
          "\\.vs-code"
        ],
        "quiet": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "git_switch_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "github_repository": {
        "owner": "pluralsight",
        "name": "git-switch-electron"
      }
    }
  }
}
